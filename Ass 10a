#include<stdio.h>
int main()
{
  int i,j,m,n,a[5][5];
  printf("enter no of rows and columns\n");
  scanf("%d%d",&m,&n);
  printf("enter elements\n");
  for(i=0;i<m;i++)
  {
  for(j=0;j<n;j++)
  scanf("%d",&a[i][j]);
  }
  printf("Transpose of the given matrix\n");
  for(i=0;i<m;i++)
  {
    for(j=0;j<n;j++)
    {
    printf(" %d ",a[j][i]);
    }
    printf("\n");
  }
  return 0;
}
