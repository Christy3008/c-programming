#include<stdio.h>
int main()
{
  double mark;
  printf("Mark obtained\n");
  scanf("%lf",&mark);
  if(mark>=90)
  printf("Grade A\n");
  else if(mark>=70)
  printf("Grade B\n");
  else if(mark>=50)
  printf("Grade C\n");
  else if(mark>=30)
  printf("Grade D\n");
  else
  printf("Failed\n");
  return 0;
}
