#include<stdio.h>
int prime(int n)
{
int i;
if(n==1)
printf("Not a prime number");
else
{
for(i=2;i<n-1;i++)
  { 
  if(n%i==0)
 {
  printf("Not a prime number\n");
  return 0;
 }
  }
printf("Number is prime\n");
}
}
int main()
{
int n;
printf("Enter number: \n");
scanf("%d",&n);
prime(n);
return 0;
}
    
